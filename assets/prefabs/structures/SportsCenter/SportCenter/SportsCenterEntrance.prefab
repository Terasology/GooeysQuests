            /** Require that left wall is hidden */
            {"condition": "StructureTemplates:IsGroundLike", "region" :{ "min": [3, 0, 6], "size": [1, 5, 4]}},
            /** Require that right wall is hidden */
            {"condition": "StructureTemplates:IsGroundLike", "region" :{ "min": [-3, 0, 6], "size": [1,  5, 4]}},
            /** Require that back wall is hidden */
